#include<iostream>
using namespace std;
int main()
{
int n;
float net_salary;
cout<<"ENTER THE NUMBER OF EMPLOYEES";
cin>>n;
for(int i=1;i<=n;i++)
{int basic_salary;
    cout<<"ENTER THEIR BASIC SALARIES";
    cin>> basic_salary;
    net_salary=basic_salary+((12.0/100)*basic_salary);
    cout<<"the net salary is:"<<net_salary;}
return 0;
}
